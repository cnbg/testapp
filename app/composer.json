    "guzzlehttp/guzzle": "^7.0",
    "vlucas/phpdotenv": "5.4.x-dev"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "phpunit/phpunit": "^9.5.20",
    "mockery/mockery": "^1.5"
  }
}
